{
  "name": "@pcuci/ranges",
  "version": "0.0.1",
  "description": "keep track of ranges",
  "main": "src/index.js",
  "scripts": {
    "test": "ava",
    "lint": "standard .",
    "release": "DEBUG=* semantic-release-gitlab",
    "publish": "npm-publish-git-tag"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/pcuci/ranges.git"
  },
  "keywords": [
    "ranges",
    "collections"
  ],
  "author": "paulcu@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pcuci/ranges/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/pcuci/ranges#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "ava": "^1.1.0",
    "husky": "^1.3.1",
    "npm-publish-git-tag": "^3.0.3",
    "semantic-release-gitlab": "^7.1.3",
    "standard": "^12.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "ava": {
    "files": [
      "test/**/*spec.js"
    ],
    "sources": [
      "**/*.js",
      "!test/**"
    ],
    "failWithoutAssertions": true
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {}
}
