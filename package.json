    "access": "public"
  },
  "homepage": "https://github.com/pcuci/ranges#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "@semantic-release/gitlab-config": "^5.0.0",
    "ava": "^1.1.0",
    "husky": "^1.3.1",
    "npm-publish-git-tag": "^3.0.3",
    "semantic-release": "^15.13.3",
    "standard": "^12.0.1"
  },
  "husky": {
    "hooks": {
